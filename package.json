{
  "name": "twilio-function-generators",
  "version": "0.0.1",
  "packageManager": "yarn@4.9.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/zingdevlimited/twilio-function-generators.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc --build",
    "watch": "yarn clean && tsc --watch",
    "generate-package": "yarn build && yarn pack --out artifacts/%s-%v.tgz",
    "generate-key": "node scripts/generate-key.js"
  },
  "peerDependencies": {
    "@twilio-labs/serverless-runtime-types": "^4.0.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@twilio-labs/serverless-runtime-types": "^4.0.1",
    "@types/jsonwebtoken": "^9",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0",
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "jsonwebtoken": "^9.0.2",
    "oauth4webapi": "^3.5.0",
    "ulidx": "^2.4.1"
  }
}
